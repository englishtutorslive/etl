<% if user_signed_in? %>
  <% if @teachers.any? %>
    <% @teachers.each do |p| %>
      <% if p.profile != nil %>
        <h4> Name: <%= link_to "#{p.profile.name}", student_path(p) student.teachers << p %> </h4>
        <h4> Bio: <%= p.profile.bio %> </h4>
      <% end %>
    <% end %>
  <% end %>
<% end %>
