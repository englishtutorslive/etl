<!DOCTYPE html>
<html>
  <head>
    <title>Etl</title>
      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
      <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
  </head>

  <body>

    <% if user_signed_in? %>
      Welcome, <%= current_user.email %>
      <%= link_to "Log out", destroy_user_session_path, method: :delete %>
    <% end %>

    <% if teacher_signed_in? && !user_signed_in? %>
      Welcome, <%= current_teacher.email %>
      <%= link_to "Teacher log out", destroy_teacher_session_path, method: :delete %>
    <% end %>

    <% if !teacher_signed_in? && !user_signed_in? %>
      <%= link_to "Student sign in", new_user_session_path %>
      <%= link_to "Teacher sign in", new_teacher_session_path %>
    <% end %>

    <%= render 'layouts/header' %>
    <%= yield %>


  </body>
</html>
